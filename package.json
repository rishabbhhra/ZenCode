{
	"name": "zencode",
	"displayName": "ZenCode",
	"description": "AI-powered stress detection for developers inside VS Code.",
	"version": "1.0.0",
	"publisher": "Aranyaka",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"contributes": {
		"commands": [
			{
				"command": "zencode.start",
				"title": "Start ZenCode"
			}
		]
	},
	"activationEvents": [
		"onStartupFinished",
		"workspaceContains:**/*.js"
	],
	"main": "./src/extension.js",
	"scripts": {
		"package": "vsce package",
		"publish": "vsce publish"
	},
	"devDependencies": {
		"vscode": "^1.1.34"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/scintillating-Shruti-Pandey/ZenCode"
	},
	"dependencies": {
		"axios": "^1.8.4"
	}
}
